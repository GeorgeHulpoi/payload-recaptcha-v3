{
	"name": "payload-recaptcha-v3",
	"version": "3.0.0-beta.2",
	"description": "This library implements a collection protection in Payload CMS using Google reCAPTCHA v3.",
	"license": "MIT",
	"type": "module",
	"keywords": [
		"payload",
		"cms",
		"plugin",
		"recaptcha",
		"v3"
	],
	"bugs": {
		"url": "https://github.com/GeorgeHulpoi/payload-recaptcha-v3/issues"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"packageManager": "pnpm@8.15.7",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./withPlugin": {
			"import": "./dist/withPlugin.js",
			"require": "./dist/withPlugin.js"
		}
	},
	"scripts": {
		"build": "pnpm clean && pnpm build:types && pnpm build:swc",
		"build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
		"build:types": "tsc --emitDeclarationOnly --outDir dist",
		"lint": "eslint ./src/**/*",
		"lint:fix": "eslint --fix --ext .ts,.tsx ./src/**/*",
		"clean": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
		"prepare": "husky install",
		"dev": "next dev ./dev",
		"dev:build": "next build ./dev",
		"dev:start": "next start ./dev",
		"generate:types": "cross-env PAYLOAD_CONFIG_PATH=dev/src/payload/payload.config.ts payload generate:types",
		"test": "playwright test"
	},
	"author": {
		"name": "George-Valentin Hulpoi",
		"url": "https://gvhdev.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GeorgeHulpoi/payload-recaptcha-v3.git"
	},
	"dependencies": {
		"qs": "^6.12.1"
	},
	"peerDependencies": {
		"next": "15.0.0-canary.104",
		"payload": "3.0.0-beta.90"
	},
	"devDependencies": {
		"@payloadcms/db-mongodb": "3.0.0-beta.90",
		"@payloadcms/next": "3.0.0-beta.90",
		"@payloadcms/richtext-lexical": "3.0.0-beta.90",
		"@playwright/test": "^1.45.1",
		"@swc/cli": "0.3.12",
		"@swc/jest": "0.2.36",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.14.2",
		"@types/qs": "^6.9.15",
		"@types/react": "npm:types-react@19.0.0-beta.0",
		"@types/react-dom": "npm:types-react-dom@19.0.0-beta.0",
		"copyfiles": "^2.4.1",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react": "^7.34.2",
		"globals": "^15.4.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"lint-staged": "^14.0.0",
		"next": "15.0.0-canary.104",
		"payload": "3.0.0-beta.90",
		"prettier": "^3.3.2",
		"react": "19.0.0-rc-6230622a1a-20240610",
		"react-dom": "19.0.0-rc-6230622a1a-20240610",
		"rimraf": "^5.0.7",
		"ts-node": "^10.9.2",
		"turbo": "^2.0.6",
		"typescript": "^5",
		"typescript-eslint": "^7.13.0"
	},
	"lint-staged": {
		"*.ts": "eslint src --cache --fix",
		"*.--write": "prettier --write"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "npm:types-react@19.0.0-rc.0",
			"@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
		}
	},
	"overrides": {
		"@types/react": "npm:types-react@19.0.0-rc.0",
		"@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
	}
}
